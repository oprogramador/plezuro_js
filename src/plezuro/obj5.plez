$Animal = Module.create(%(
    'name': 'Animal',
    'methods': %(
        'init': {
            @age = 0
        },
        'say': {
            'I am '+@age
        },
        '~': {
            @age++
        },
        '+', {
            @age += first
        }
    )
));
$a = Animal.new;
output = '';
output += a.say;
a~;
output += a.say;
a + 3;
