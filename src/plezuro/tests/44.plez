$x = r'[a-z]';
$b = x =~ 'abc';
$b1 = 'dfg' =~ x;
$b2 = 'dfg' !~ x;
$b3 = '12' !~ x;
$b4 = x !~ 'xxx';
$res = [x, b, b1, b2, b3, b4];
res << 'gh-kk' =~ r'[a-z]'
    << 'gh-kk' =~ r'^[a-z]$'
    << 'gh-kk' =~ r'^[a-z]+-[a-z]+$'
    << 'gh-kk' =~ r'^[a-z]{2}-[a-z]{2}$'
    << 'gh-kko' =~ r'^[a-z]{2}-[a-z]{2}$'
    << '\\' =~ r'^\\$'
    << '\\' =~ r'\\'
    << '\\' =~ r"\\"
    << '0+9' =~ r"^[0-9]\+[0-9]$"
    << '0+97' =~ r"^[0-9]\+[0-9]$"
    << 'a"k' =~ r"^a""k"
    << 'ak' =~ r"^a""k"
    << 'a"k' =~ r'^a""k'
    << 'a"k' =~ r'^a"k'
    << "a'k" =~ r'^a''k'
    << "ak" =~ r'^a''k'
    << "a'k" =~ r"^a''k"
    << "a'k" =~ r"^a'k"
;
res
